    "    '*': 'multiplicação',\n",
    "    '/': 'divisão'\n",
    "}\n",
    "\n",
    "while True:\n",
    "    os.system('clear')\n",
    "    i = 0\n",
    "    for op, name in operations.items():\n",
    "        print(i, ':', name)\n"
   ]
